<h1>Listing program_of_studies</h1>

<table>
  <thead>
    <tr>
      <th>Program</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_of_studies.each do |program_of_study| %>
      <tr>
        <td><%= program_of_study.program %></td>
        <td><%= program_of_study.section.section %></td>
        <td><%= link_to 'Show', program_of_study %></td>
        <td><%= link_to 'Edit', edit_program_of_study_path(program_of_study) %></td>
        <td><%= link_to 'Destroy', program_of_study, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program of study', new_program_of_study_path %>
